{
    "name": "security-group-maas",
    "rules": [
        {
            "name": "tcp-all",
            "protocol": "tcp",
            "remote_ip_prefix": "0.0.0.0/0",
            "port_range_max": 65535,
            "port_range_min": 1
        },
        {
            "name": "udp-all",
            "protocol": "udp",
            "remote_ip_prefix": "0.0.0.0/0",
            "port_range_max": 65535,
            "port_range_min": 1
        },
        {
            "name": "icmp",
            "remote_ip_prefix": "0.0.0.0/0",
            "protocol": "icmp"
        }
    ]
}